#!/bin/sh

launch_once() {
    if [ ! "$(pgrep -x "$1")" ]; then
        eval "$arg"
    fi
}

if [ $# -gt 0 ]; then
    for arg in "$@"; do
        launch_once "$arg" &
    done
else
    program=$(exec_program --print-cmd --prompt="Single Instance")

    if [ "$program" ]; then
        launch_once "$program"
    fi
fi

